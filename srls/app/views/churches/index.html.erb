<h1>Listing churches</h1>

<table>
  <tr>
    <th>Nombre</th>
    <th>Zona</th>
    <th>Vicaria</th>
    <th>Codigo</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @churches.each do |church| %>
  <tr>
    <td><%= church.nombre %></td>
    <td><%= church.zona %></td>
    <td><%= church.vicaria %></td>
    <td><%= church.codigo %></td>
    <td><%= link_to 'Show', church %></td>
    <td><%= link_to 'Edit', edit_church_path(church) %></td>
    <td><%= link_to 'Destroy', church, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Church', new_church_path %>
